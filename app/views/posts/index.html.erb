


<% @posts.each do |post| %>
  <div class="post_wrapper">
    <h2 class="title"><%= link_to post.title, post %></h2>
    <p class="date"><%= post.created_at.strftime("%B, %d, %Y") %></p>
  </div>
<% end %>

<%= will_paginate @posts, renderer: BootstrapPagination::Rails, inner_window: 1, outer_window: 0, class: "center" %>
